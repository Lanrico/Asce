{
  "name": "cliper",
  "description": "cliper for bilibili",
  "version": "1.0",
  "manifest_version": 2,
  //popup
  "browser_action":{
    "default_popup": "popup.html",
    "default_icon": "popup.png"
  },
  "permissions": ["tabs"],
  "content_scripts": [
  {
    "js": ["js/tet.js"],
    // 在什么情况下使用该脚本
    "matches": ["<all_urls>"],
    // 什么情况下运行【文档加载开始】
    "run_at": "document_start"
  }
],
    "background": {
    // "page": "background.html",
    "scripts": ["js/background.js"],
    "persistent": true
    },
}
